#!/bin/bash

case "$1" in
    "remove" | "purge" | "abort-install" | "disappear")
        service key-keeper stop

        update-rc.d key-keeper remove
        ;;

    "upgrade" | "failed-upgrade" | "abort-upgrade")
        ;;

    *)
        echo "postrm called with unknown argument '$1'" >&2
        exit 1
        ;;
esac

exit 0

